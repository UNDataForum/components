{
  "name": "@undataforum/components",
  "description": "React components for United Nations World Data Forum websites.",
  "version": "0.35.2",
  "author": {
    "name": "Thilo Maier",
    "email": "maier@un.org"
  },
  "homepage": "https://components.undataforum.org",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/undataforum/components.git",
  "keywords": [
    "react",
    "components",
    "styled-components",
    "styled-system"
  ],
  "scripts": {
    "start": "start-storybook -p 8000 -c ./config/storybook -s ./static",
    "now-build": "rm -rf ./storybook-static && build-storybook -c ./config/storybook -s ./static",
    "build": "rm -rf ./dist && cross-env NODE_ENV=production babel ./src -d ./dist --ignore 'src/**/*.stories.js'",
    "clean": "rm -rf ./node_modules",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@styled-system/theme-get": "^5.0.6",
    "prism-react-renderer": "^0.1.6",
    "prop-types": "^15.7.2",
    "react-helmet": "^5.2.0",
    "rebass": "^3.1.1",
    "styled-system": "^5.0.6"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-storysource": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/theming": "^5.0.11",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.1",
    "cross-env": "^5.2.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
