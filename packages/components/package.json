{
  "name": "@undataforum/components",
  "description": "React components for United Nations World Data Forum websites.",
  "version": "0.44.0",
  "author": {
    "name": "Thilo Maier",
    "email": "maier@un.org"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/UNDataForum/components/tree/master/packages/components#readme",
  "repository": "https://github.com/UNDataForum/components/tree/master/packages/components",
  "keywords": [
    "react",
    "components",
    "styled-components",
    "styled-system"
  ],
  "scripts": {
    "start": "start-storybook -p 8000 -s ./static",
    "build": "rm -rf ./dist && cross-env NODE_ENV=production babel ./src -d ./dist --ignore 'src/**/*.stories.js'",
    "build-storybook": "rm -rf ../../public && build-storybook -s ./static -o ../../public",
    "purge": "rm -rf ./node_modules",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "@emotion/core": "^10.0.15",
    "@rebass/preset": "^4.0.0",
    "emotion-theming": "^10.0.14",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.7.2",
    "rebass": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@storybook/addon-a11y": "5.2.4",
    "@storybook/addon-actions": "5.2.4",
    "@storybook/addon-console": "1.2.1",
    "@storybook/addon-knobs": "5.2.4",
    "@storybook/addon-storysource": "5.2.4",
    "@storybook/react": "5.2.4",
    "babel-loader": "8.0.6",
    "cross-env": "6.0.3",
    "react": "16.10.2",
    "react-dom": "16.10.2"
  },
  "publishConfig": {
    "access": "public"
  }
