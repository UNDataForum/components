{
  "version": 2,
  "name": "components",
  "alias": ["components.undataforum.org"],
  "github": {
    "autoAlias": false
  },
  "builds": [
    {
      "src": "packages/components/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "storybook-static" }
